generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  telefono  String?
  creadoEn  DateTime @default(now())
}

model Broker {
  id      Int    @id @default(autoincrement())
  nombre  String
  email   String @unique
}



